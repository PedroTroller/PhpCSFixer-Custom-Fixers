---
plugins:
  -
    - '@semantic-release/commit-analyzer'
    - releaseRules:
        - type: BREAKING CHANGE
          release: major
        - type: feat
          release: minor
        - type: fix
          release: fix
        - scope: deps
          release: minor
        - scope: deps-dev
          release: false
        - type: refactor
          release: false
        - scope: chore
          release: false
  - '@semantic-release/release-notes-generator'
  - '@semantic-release/github'
