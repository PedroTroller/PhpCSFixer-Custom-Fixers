---
plugins:
  -
    - '@semantic-release/commit-analyzer'
    - releaseRules:
        - type: feat
          release: minor
        - type: fix
          scope: deps
          release: minor
        - type: fix
          scope: deps-dev
          release: patch
        - type: refactor
          release: patch
        - scope: chore
          release: false
  - '@semantic-release/release-notes-generator'
  - '@semantic-release/github'
