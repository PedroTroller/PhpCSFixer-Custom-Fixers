{
    "name": "pedrotroller/php-cs-custom-fixer",
    "description": "PHP-CS-FIXER : my custom fixers",
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "friendsofphp/php-cs-fixer": "~2.11"
    },
    "require-dev": {
        "mustache/mustache": "^2.12",
        "phpspec/phpspec": "^3.4",
        "webmozart/assert": "^1.3"
    },
    "autoload": {
        "psr-4": { "PedroTroller\\CS\\": "src/PedroTroller/CS/" }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "scripts": {
        "tests": [
            "tests\\Runner::run",
            "phpspec run -fpretty"
        ]
    }
}
